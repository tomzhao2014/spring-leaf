<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qiktone.repository.HostRepository">

    <select id="list" resultType="Host" >
        select * from host
    </select>

    <select id="getById"  resultType="Host" parameterType="java.lang.Long">
        select * from host where id = ${id}
    </select>


</mapper>