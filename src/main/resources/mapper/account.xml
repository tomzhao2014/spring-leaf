<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qiktone.repository.AccountRepository">
    <select id="list" resultType="Account" >
        select * from company
    </select>

    <select id="getById"  resultType="Account" parameterType="java.lang.Long">
        select * from account where id = ${id}
    </select>

</mapper>